<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Ticker">
        <attr name="hoursFormat" format="enum">
            <enum name="hours12" value="12"/>
            <enum name="hours24" value="24"/>
        </attr>
        <attr name="minutesInterval" format="integer"/>
        <attr name="shouldAmSelected" format="boolean"/>
        
        <attr name="selectedNumberColor" format="color" defaultValue="#000000"/>
        <attr name="otherNumberColor" format="color" defaultValue="#808080"/>
    </declare-styleable>
</resources>
